{
  "name": "ts-soundboard",
  "productName": "TitusStudios Soundboard",
  "version": "1.6.3",
  "description": "Simple soundboard made in HTML5, and built using Electron.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "TitusStudiosMediaGroup",
    "email": "titusstudiosmediagroup@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "soundboard"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "app-builder": "^6.2.2",
    "app-builder-bin": "^3.5.6",
    "app-builder-lib": "^22.5.1",
    "build": "^0.1.4",
    "electron-builder": "^22.5.1",
    "electron-squirrel-startup": "^1.0.0",
    "js-yaml": "^3.13.1",
    "nord": "^0.2.1",
    "uglify-js": "^3.9.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "electron": "8.2.3"
  }
}
